{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon -r dotenv/config",
    "start-ci": "nohup nodemon -r dotenv/config </dev/null &",
    "debug": "nodemon -r dotenv/config --inspect",
    "tests": "npm run unit-tests && npm run integration-tests",
    "clearJest": "jest --clearCache",
    "unit-tests": "npm run clearJest&& jest src/core/tests/*.test.ts src/modules/*/tests/unit/*.test.ts --forceExit",
    "unit-tests-coverage": "jest src/core/tests/*.test.ts src/modules/*/tests/unit/*.test.ts --coverage --forceExit",
    "integration-tests": "npm run clearJest&& jest src/modules/*/tests/integration/*.test.ts --forceExit",
    "integration-tests-coverage": "jest src/modules/*/tests/integration/*.test.ts --coverage --forceExit",
    "linter": "tslint -c ./tslint.json './src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "bunyan-express-serializer": "^1.0.0",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "easygraphql-format-error": "0.0.3",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "helmet": "^4.1.0",
    "install": "^0.13.0",
    "jest": "^26.4.2",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.10.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "npm": "^6.14.8",
    "stream": "0.0.2",
    "supertest": "^4.0.2",
    "uuid": "^8.3.0"
  },
